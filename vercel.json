{
    "version": 2,
    "builds": [
      {
        "src": "api/webhook.py",
        "use": "@vercel/python",
        "config": {
          "includeFiles": ["config.json"],
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/webhook",
        "dest": "api/webhook.py"
      }
    ]
  }